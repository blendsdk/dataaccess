{
    "name": "@blendsdk/commonutils",
    "version": "0.0.15",
    "description": "Common utilities to use in Nodejs",
    "author": "Gevik Babakhani <gevik@blendjs.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/blendsdk/dataaccess"
    },
    "scripts": {
        "watch": "../../node_modules/.bin/watch 'npm run build' ./src",
        "build": "npm run clean && npm run tslint && npm run build:lib",
        "build:lib": "../../node_modules/.bin/tsc -p ./",
        "clean": "../../node_modules/.bin/rimraf .build dist",
        "tslint": "../../node_modules/.bin/tslint --config ../../tslint.json --project ./tsconfig.json"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "keywords": [
        "typescript",
        "framework",
        "library",
        "express"
    ],
    "dependencies": {
        "express": "^4.16.4",
        "express-validator": "^5.3.1"
    },
    "devDependencies": {
        "@types/express": "^4.16.0",
        "tslint": "^5.12.0",
        "typescript": "^3.2.2"
    }
}
